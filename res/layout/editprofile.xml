<?xml version="1.0" encoding="utf-8"?>
<TableLayout
android:id="@+id/widget132"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:orientation="vertical"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<LinearLayout
android:id="@+id/widget133"
android:layout_width="wrap_content"
android:layout_height="20px"
android:orientation="vertical"
>
</LinearLayout>
<TextView
android:id="@+id/widget135"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Edit a Profile"
android:textSize="20sp"
android:typeface="serif"
android:gravity="center"
>
</TextView>
<LinearLayout
android:id="@+id/widget134"
android:layout_width="wrap_content"
android:layout_height="30px"
android:orientation="vertical"
>
</LinearLayout>
<LinearLayout
android:id="@+id/widget143"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:orientation="vertical"
>
<LinearLayout
android:id="@+id/widget140"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_gravity="center_horizontal"
>
<TextView
android:id="@+id/widget145"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Name: "
android:textSize="16sp"
android:typeface="serif"
android:layout_gravity="center_vertical"
>
</TextView>
<Spinner
android:id="@+editprofile/name"
android:layout_width="170px"
android:layout_height="wrap_content"
android:layout_gravity="center_horizontal"
>
</Spinner>
</LinearLayout>
</LinearLayout>
<LinearLayout
android:id="@+id/widget139"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:orientation="vertical"
>
<LinearLayout
android:id="@+id/widget136"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_gravity="center_horizontal"
>
<TextView
android:id="@+id/widget137"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Volume: "
android:textSize="16sp"
android:typeface="serif"
android:layout_gravity="center_vertical"
>
</TextView>
<SeekBar
android:layout_width="135px"
android:layout_height="wrap_content"
android:id="@+editprofile/volume"
android:max="100"
android:progress="50"
>
</SeekBar>
<TextView 
android:layout_width="35px"
android:layout_height="wrap_content"
android:textSize="16sp"
android:typeface="serif"
android:layout_gravity="center_vertical"
android:id="@+editprofile/volumevalue"
android:text=" 50"
/>
</LinearLayout>
</LinearLayout>
<LinearLayout
android:id="@+id/widget143"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:orientation="vertical"
>
<LinearLayout
android:id="@+id/widget140"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_gravity="center_horizontal"
>
<TextView
android:id="@+id/widget141"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Vibrate: "
android:textSize="16sp"
android:typeface="serif"
android:layout_gravity="center_vertical"
>
</TextView>
<CheckBox
android:id="@+editprofile/vibrate"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="On/Off"
android:textSize="16sp"
android:typeface="serif"
android:checked="true"
>
</CheckBox>
</LinearLayout>
</LinearLayout>
<LinearLayout
android:id="@+id/widget148"
android:layout_width="wrap_content"
android:layout_height="20px"
android:orientation="vertical"
>
</LinearLayout>
<LinearLayout
android:id="@+id/widget153"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:orientation="vertical"
>
<LinearLayout
android:id="@+id/widget149"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_gravity="center_horizontal"
>
<Button
android:id="@+editprofile/clearButton"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Clear"
>
</Button>
<Button
android:id="@+editprofile/exitButton"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Cancel"
>
</Button>
<Button
android:id="@+editprofile/editButton"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text=" Edit "
>
</Button>
</LinearLayout>
</LinearLayout>
</TableLayout>
